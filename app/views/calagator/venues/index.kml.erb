<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.2">
  <Document>
  <% for venue in @venues -%>
  <% next if venue.latitude.nil? -%>
  <Placemark>
    <name><%= escape_once(venue.title) %></name>
    <description><%= cdata_section(venue.description) %></description>
    <Point>
      <coordinates><%= venue.longitude %>,<%= venue.latitude %></coordinates>
    </Point>
  </Placemark>
  <% end -%>
  </Document>
</kml>